@{
	ViewData["Title"] = "غیر فعال شدن صلاحیت ها";
	@model List<my_projects.Models.DisabledCredentialsViewMode>;
		
}

<div class="container he-100">
	<h5 class="align-right fs-s font-weight-bold mb-3"> گزارش غیر فعال شدن صلاحیت های پرونده جاری</h5>
	<table id="tbGrid" class="table hover text-center fs-es mt-3">
		<thead>
			<tr>
				<th scope="col">تاریخ شروع</th>
				<th scope="col">تاریخ پایان</th>
				<th scope="col">رشته سازمانی</th>
				<th scope="col">نوع صلاحیت</th>
				<th scope="col"> سهم و کار</th>
				<th scope="col"> تاریخ ثبت </th>
				<th scope="col"> توضیح </th>
			</tr>
		</thead>
		<tbody id="">
			@{
				foreach (var item in Model)
				{
					<tr>
						<td>@item.StartDate</td>
						<td>@item.EndDate</td>
						<td>@item.OrganizationalField</td>
						<td>@item.TypeOfCompetence</td>
						<td>@item.ShareAndWork</td>
						<td>@item.DateOfRegistration</td>
						<td>@item.Description</td>
					</tr>
				}
			}

		</tbody>
	</table>

</div>
@await Html.PartialAsync("~/Views/Scripts/_GridScripts.cshtml")